<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.app.mapper.ClothesMapper">

	<select id="selectAll" resultType="Clothes">
		select*from clothes
	</select>

	<select id="slectById" resultType="Clothes">
		select*from clothes
		where
		id=#{id}
	</select>

	<select id="selectByCategory" resultType="Clothes">
		select*from clothes
		where category_id=#{categoryId}
	</select>

</mapper>